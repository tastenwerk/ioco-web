#webpages-plugin.content-padding
  
  .iokit-webpages-tree.second-sidebar

  #webpages-form-container.second-main-container

script
  var webElementId = !{typeof(webElementId) !== 'undefined' && webElementId ? '\''+webElementId+'\'' : 'null'};
  $.getScript('/webpages/tree', function(){
    $('.iokit-webpages-tree').webpagesTree( {completed: function( treeElem ){
      setTimeout(function(){
        if( webElementId )
          if( $('li#'+webElementId).length )
            $('li#'+webElementId+' a.link-trigger').click();
          else
            $.getJSON( '/documents/'+webElementId, function( data ){
              console.log(data);
              $('#webpages-form-container').load( '/webpages/'+webElementId+'/edit', function(){
                ko.applyBindings( new WebpageModel( data ), $('#webpage-'+self._id+'-form').get(0) );
              });
            });
        },500);
      }
    });
  });