ioco.main.html(<%- JSON.stringify(partial( 'articles/form.jade', { article: web_element, moment: moment, currentUser: currentUser } )) %>);
ioco.main.find('#editor').kendoEditor({
  encoded: false,
  tools: [
    "bold",
    "italic",
    "underline",
    "strikethrough",
    "foreColor",
    "backColor",
    "justifyLeft",
    "justifyCenter",
    "justifyRight",
    "justifyFull",
    "insertUnorderedList",
    "insertOrderedList",
    "indent",
    "outdent",
    "formatBlock",
    "createLink",
    "unlink",
    "insertImage",
    "viewHtml"
  ]
});

ioco.main.find('#article_name').on('keyup', function(e){
  var v = $(this).val();
  if( v.length < 1 )
    v = $.t('articles.new');

  $(this).closest('.ioco-main-content-inner').find('.article-title').text( v );
});

$("#article_veranstaltung_ac").kendoAutoComplete({
  minLength: 2,
  dataTextField: "name",
  dataSource: {
    type: "json",
    serverFiltering: true,
    serverPaging: true,
    pageSize: 20,
    transport: {
        read: ioco.host.native+'/veranstaltungen'
    }
  },
  select: function( e ){
  console.log($(e.item).find('.selected-val').val() );
    $('#article_veranstaltung').val( $(e.item).find('.selected-val').val() );
  },
  template: "#= name # - #= moment(firstDate).format('DD.MM.YYYY') # - #= location.name ? location.name : '' # <input type='hidden' value='#= _id #' class='selected-val' />"
});


setTimeout( function(e){
  ioco.main.find('#article_name').focus();
}, 500)


