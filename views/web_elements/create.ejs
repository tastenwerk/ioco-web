<% if( flash.notice && flash.notice.length > 0 ){ %>

  var webElement = <%- JSON.stringify( webElement ) %>;
  <% if( ioco.view.lookup( webElement._subtype.toLowerCase()+'s/create.ejs' ) ){ %>
    <%- JSON.stringify( partial( webElement._subtype.toLowerCase()+'s/create.ejs' ) ) %>
  <% } %>
  $('.ioco-<%= webElement._subtype.toLowerCase()+'s' %>-tree .ioco-tree-list')

<% } %>
  
ioco.notify( <%- JSON.stringify( flash ) %> );